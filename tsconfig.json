{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "strict": true,
    "declaration": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "removeComments": true,
    "stripInternal": true,
    "strictPropertyInitialization": false
  }
}
